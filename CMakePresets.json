{
  "version": 3,
  "configurePresets": [
    {
      "name": "default",
      "displayName": "Default Configuration",
      "description": "Standard build with priority queue and standard algorithm",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "USE_STACK_QUEUE": "OFF",
        "MODIFIED_ALPHA_TRAVERSABILITY": "OFF"
      }
    },
    {
      "name": "alternative-trav",
      "displayName": "Alternative Traversability",
      "description": "Build with alternative alpha traversability criteria",
      "binaryDir": "${sourceDir}/build-alt",
      "cacheVariables": {
        "USE_STACK_QUEUE": "OFF",
        "MODIFIED_ALPHA_TRAVERSABILITY": "ON"
      }
    },
    {
      "name": "stack-queue",
      "displayName": "Stack Queue",
      "description": "Build with stack as queue instead of priority queue",
      "binaryDir": "${sourceDir}/build-stack",
      "cacheVariables": {
        "USE_STACK_QUEUE": "ON",
        "MODIFIED_ALPHA_TRAVERSABILITY": "OFF"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "configurePreset": "default"
    },
    {
      "name": "alternative-trav",
      "configurePreset": "alternative-trav"
    },
    {
      "name": "stack-queue",
      "configurePreset": "stack-queue"
    }
  ]
}